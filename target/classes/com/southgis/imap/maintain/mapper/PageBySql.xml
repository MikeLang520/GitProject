<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.southgis.imap.maintain.mapper.PageBySql">
 <select id="queryBySql" parameterType="map" resultType="SmsInfo">
        select CID,Caccount,Ccompany,Cmobile,Cmobile1111,Caddress,Cprovince,Czip,Ctel,Cfax,Cname,
		Cpassword,Cquestion,Canswer,Cnote,createtime from SmsInfo limit #{currIndex} , #{pageSize}
</select>

</mapper>